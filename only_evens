n = input("Enter count of numbers: ")
n = int(n)

count = 1
numbers = []
con = 0

while count <= n:
    number = input("Enter number: ")
    number = int(number)

    numbers = numbers + [number]

    count += 1
print(numbers)

for num in numbers:
    
    if num % 2 == 0:
        con += 1
print("The count of events numbers is %d" % con)
